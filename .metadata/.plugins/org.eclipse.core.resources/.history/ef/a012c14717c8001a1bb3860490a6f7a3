package persone;

public class CartaServizi {
	
	private static int cod = 0;
	private int cod_identificativo;

	public CartaServizi() {
		this.cod_identificativo = cod;
		cod++;
	}

	public int getCod_identificativo() {
		return cod_identificativo;
	}

	public void setCod_identificativo(int cod_identificativo) {
		this.cod_identificativo = cod_identificativo;
	}

}

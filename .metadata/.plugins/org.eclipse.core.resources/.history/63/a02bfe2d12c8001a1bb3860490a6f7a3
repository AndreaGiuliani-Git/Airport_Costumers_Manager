package tester;

import persone.Flyer;
import persone.FrequentFlyer;
import persone.Sesso;
import sale.SalaAttesa;
import sale.SalaVip;

public class Tester {
	
	public static void main(String[] args) {
		
		SalaAttesa s1 = new SalaAttesa(10);
		SalaAttesa s2 = new SalaAttesa(5);
		SalaVip s3 = new SalaVip(5);
		
		Flyer f1 = new Flyer("Giacomo", "Rossi", 30, Sesso.UOMO, 10120);
		Flyer f2 = new Flyer("Giacomo", "Rossi", 30, Sesso.UOMO, 10120);
		FrequentFlyer f3 = new FrequentFlyer("Giacomo", "Rossi", 30, Sesso.UOMO, 10120, 300000);
		FrequentFlyer f4 = new FrequentFlyer("Giacomo", "Rossi", 30, Sesso.UOMO, 10120, 274000);
		FrequentFlyer f5 = new FrequentFlyer("Giacomo", "Rossi", 30, Sesso.UOMO, 10120, 20000);
		
		f1.stampaInfo();
		f2.stampaInfo();
		f3.stampaInfo();
		f4.stampaInfo();
		f5.stampaInfo();
		
	}

}
